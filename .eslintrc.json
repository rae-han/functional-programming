{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "newline-per-chained-call": [
      "error",
      { "ignoreChainWithDepth": 0 }
    ],
    "no-console": "off",
    "prettier/prettier": "off"
  }
}
